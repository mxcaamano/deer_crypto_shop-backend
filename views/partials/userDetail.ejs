<div class="container py-3">
  <h2>Administracion de Usuarios 🙎‍♂️🙎‍♀️</h2>
  <div class="container my-3">
    <img class="mb-3" src=<%= user.imgURL %> alt="" width="80" height="80">
        <div class="btn-group">
          <form action="/usuarios/<%= user.id %>?_method=DELETE" method="post">
              <button type="submit" class="mx-4 p-2 rounded-0 btn btn-danger">Eliminar</button>
          </form>
        </div></br>
        <span>Nombre</span>
        <span class="form-control w-25 p-1 mb-3 mt-1"><%= user.name %></span>
        <span>Correo Electrónico</span>
        <span class="form-control w-25 p-1 mb-3 mt-1"><%= user.email %></span>
        <span>Dirección</span>
        <span class="form-control w-25 p-1 mb-3 mt-1"><%= user.address %></span>
        <span>Teléfono</span>
        <span class="form-control w-25 p-1 mb-3 mt-1"><%= user.phone %></span>
        <span>Edad</span>
        <span class="form-control w-25 p-1 mb-3 mt-1"><%= user.age %></span><br>
  </div>
</div>